{
  "extends": "../../tsconfig.base.json",
  "include": ["**/*.ts", "**/*.tsx", "**/*.d.ts"],
  "compilerOptions": {
    "moduleResolution": "Bundler",
    "types": ["vite/client"],
    "paths": {
      "@/*": ["./src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "baseUrl": ".",
    "composite": true,
    "outDir": "./dist"
  },
  "exclude": ["../styles"]
}
